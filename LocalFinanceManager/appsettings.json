{
  "ConnectionStrings": {
    "Default": "Data Source=localfinancemanager.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ImportOptions": {
    "BatchSize": 1000,
    "MaxFileSizeMB": 100,
    "TimeoutSecondsPerBatch": 60,
    "FuzzyMatchThreshold": 0.65
  }
}
